platform :ios, '12.0'

target 'Traktor' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings! 

  # Pods for Carromate
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'Charts'
  pod 'Crashlytics'
  pod 'Fabric'
  pod 'Firebase'
  pod 'Firebase/Analytics'
  pod 'Firebase/Performance'
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  pod 'GoogleSignIn'
  pod 'HockeySDK'
  pod 'Loaf'
  pod 'NotificationBannerSwift'
  pod 'ViewAnimator'
  pod 'ReachabilitySwift'
  pod 'SkeletonView'
  pod 'SVProgressHUD'
  pod 'SwiftyBeaver'
  pod 'SDWebImage'
  pod 'TextFieldEffects'
  pod 'TraktKit'
  pod "TMDBSwift"
  pod 'Willow'
  pod 'YoutubeKit'
  pod 'FLEX', :configurations => ['Debug']

  target 'TraktorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TraktorUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
